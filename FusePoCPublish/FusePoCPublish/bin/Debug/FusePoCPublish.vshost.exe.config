<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="SayHiBinding" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8181/cxf/pocesb/webservices/helloService"
                binding="basicHttpBinding" bindingConfiguration="SayHiBinding"
                contract="EsbProxy.SayHiEndpoint" name="SayHiService" />
        </client>
    </system.serviceModel>
</configuration>